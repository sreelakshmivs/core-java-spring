[Unit]
Description=arrowhead-poaonboarding
After=network.target mysql.target

[Service]
WorkingDirectory=/etc/arrowhead/systems/poaonboarding
ExecStart=/usr/bin/java -Dlog4j.configurationFile=file:/etc/arrowhead/systems/poaonboarding/log4j2.xml -jar /usr/share/arrowhead/poaonboarding/arrowhead-poaonboarding.jar
TimeoutStopSec=5
Restart=on-failure
RestartSec=20s
Type=simple
User=arrowhead
Group=arrowhead

[Install]
WantedBy=default.target
